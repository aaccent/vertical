extends layout/page

block append main
  include global/layout/general/header/template
  +header-dark
  .article
    ul.breadcrumb.breadcrumb_black
      li.breadcrumb__item главная
      li.breadcrumb__item пресс-центр
      li.breadcrumb__item.breadcrumb__item_active На что обратить внимание, принимая квартиру у застройщика?
    .article__header
      .article__title На что обратить внимание, принимая квартиру у застройщика?
      .article__header__right
        .article__info
          .article__info__author Александр Горький
          .article__info__date 25 апреля 2023
        .article__share-list
          .button.button_social.article__share-list_collapsible
            img(src=require('media/static/icons/share/whatsapp.svg'))
          .button.button_social.article__share-list_collapsible
            img(src=require('media/static/icons/share/telegram.svg'))
          .button.button_social.article__share-list_collapsible
